{
  "name": "node-sso",
  "version": "1.0.0",
  "description": "> 在node.js 中实践sso 鉴权中心",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch ./ -e ts,tsx --exec ts-node app.ts",
    "build": "tsc -p tsconfig.json",
    "dev:prod": "nodemon --watch ./dist -e j,tsx --exec node ./dist/app.js",
    "prod": "rm -rf dist && yarn build && yarn dev:prod ",
    "inspect": "ts-node --inspect app.ts"
  },
