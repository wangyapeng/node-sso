    "jsonwebtoken": "^9.0.1",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-generic-session": "^2.3.0",
    "koa-jwt": "^4.0.3",
    "koa-redis": "^4.0.1",
    "koa-router": "^12.0.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
