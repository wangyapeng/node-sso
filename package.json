    "koa-redis": "^4.0.1",
    "koa-router": "^12.0.0",
    "koa-send": "^5.0.1",
    "koa-views": "^8.1.0",
    "mysql": "^2.18.1",
    "pinia": "^2.1.6",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.7",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.8",
