{
  "name": "node-sso",
  "version": "1.0.0",
  "description": "> 在node.js 中实践sso 鉴权中心",
  "main": "index.js",
  "scripts": {
    "dev": "sh ./scripts/dev-server.sh",
    "server": "sh ./scripts/start-node-server.sh",
    "build": "yarn build:node && yarn build:client && yarn build:server",
    "build:node": "tsc -p tsconfig.json",
    "build:client": "vite build --ssrManifest --outDir dist/www/client",
    "build:server": "vite build --ssr src/entry-server --outDir dist/www/server",
    "polyfill": "node ./polyfill.cjs",
    "dev:prod": "nodemon --watch ./dist -e j,tsx --exec node --experimental-specifier-resolution=node ./dist/app-bundle.js",
    "prod": "yarn build && yarn polyfill && yarn dev:prod ",
    "inspect": "ts-node --inspect app.ts"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangyapeng/node-sso.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wangyapeng/node-sso/issues"
  },
  "homepage": "https://github.com/wangyapeng/node-sso#readme",
